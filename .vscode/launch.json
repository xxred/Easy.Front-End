        {
            "type": "node",
            "request": "launch",
            "name": "Launch via node(调试配置文件)",
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--inspect-brk=9229",
                "./node_modules/@vue/cli-service/bin/vue-cli-service.js",
                "serve",
                "--port=1337"
            ],
            "autoAttachChildProcesses": true,
            "port": 9229
        }
    ]
